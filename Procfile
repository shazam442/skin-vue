# This Procfile starts the Rails server in debug mode. Allowing your IDE to
# attach to the server and debug it. Very useful!!
# May god help you if you don't use this.
rails:  rdebug-ide --host :: --port 1234 -- bin/rails server -b :: -p 3000
vue: cd client && npm run dev -- --host 0.0.0.0 --port 5173